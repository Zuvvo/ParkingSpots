<div class="p-3 mb-2 text-white">
  <div class="d-flex align-items-center justify-content-center ">
    <h1>Welcome to Parking Spots webpage</h1>
  </div>
  <div class="d-flex align-items-center justify-content-center ">
    <% unless User.logged_in? %>
      <%= render partial: "shared/slack_button" %>
    <% end %>
  </div>
</div>

<table class="table table-dark">
  <thead>
  <tr>
    <th>Spot number</th>
    <th>Book time</th>
    <th>Will be free at</th>
    <th>Booked by</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <%= render @spots %>
  </tbody>
</table>